@model List<Category>

<h1>Category list</h1>
<thead>
    <table class="table table-bordered table-striped">
        <th>
            Category Name
        </th>
        <th>
            Display Order
        </th>
</thead>
<tbody>

    @foreach (var obj in Model.OrderBy(u => u.DisplayOrder))
    {
        <tr>
            <td>@obj.Name</td>
            <td>
                @obj.DisplayOrder
            </td>
        </tr>
    }
</tbody>
</table>